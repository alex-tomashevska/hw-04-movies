seUrl\";\nimport { transformationMoviesReviews } from \"../../utils\";\n\nimport styles from \"./Review.module.css\";\n\nexport const Review = memo(() => {\n  const [reviews, setReviews] = useState([]);\n  const [isLoading, setLoading] = useState(false);\n\n  const { id } = useParams();\n\n  //const handleReviews = (review) => setReviews(review);\n  //const handleLoading = () => setLoading((prev) => !prev);\n\n  const getMovieReviews = () => {\n    setLoading(true);\n\n    axios\n      .get(url.movieReviews(id))\n      .then(({ data: { results } }) =>\n        setReviews(transformationMoviesReviews(results))\n      )\n      .finally(() => setLoading(false));\n  };\n\n  useEffect(() => {\n    getMovieReviews();\n  }, []);\n\n  if (isLoading) {\n    return <h1>Loading...</h1>;\n  }\n\n  if (!reviews.length) {\n    return <p>We dont have any reviews for this movie</p>;\n  }\n\n  return (\n    <div>\n      <ul className={styles.list}>\n        {reviews.map(({ id, author, content }) => (\n          <li className={styles.item} key={id}>\n            <p className={styles.author}> {author} </p>\n            <p className={styles.content}> {content} </p>\n          </li>\n        ))}\n      </ul>\n    </div>\n  );\n});\n",[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/Container/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/LoadMoreButton/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/Movie/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/MovieNavigation/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/Navigation/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/components/Cast/index.js",["115"],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/pages/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/pages/HomePage/index.js",[],[],"/Users/aleksandra/study/react/04/hw-04-movies/src/pages/MovieDetailsPage/